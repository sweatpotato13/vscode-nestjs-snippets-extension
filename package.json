{
    "name": "vscode-nestjs-snippets",
    "author": "Cute_Wisp <sweatpotato13@gmail.com>",
    "publisher": "cutewisp",
    "license": "MIT",
    "displayName": "Nestjs Snippets",
    "description": "This extension for adding snippets for nestjs in vscode",
    "version": "1.0.1",
    "repository": "https://github.com/sweatpotato13/vscode-nestjs-snippets-extension",
    "icon": "nestjs.jpg",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Snippets"
    ],
    "tags": [
        "nestjs",
        "typescript",
        "node",
        "backend",
        "snippet"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "typescript",
                "path": "./src/snippets.json"
            }
        ]
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "if git-branch-is dev; then commitlint -E HUSKY_GIT_PARAMS; fi"
        }
    },
    "scripts": {
        "vscode:prepublish": ""
    }
}
