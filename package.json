{
    "name": "vscode-nestjs-snippets",
    "author": "Cute_Wisp <sweatpotato13@gmail.com>",
    "publisher": "cutewisp",
    "license": "MIT",
    "displayName": "Nestjs Snippets",
    "description": "This extension for adding snippets for nestjs in vscode",
    "version": "1.0.0",
    "repository": "https://github.com/sweatpotato13/vscode-nestjs-snippets-extension",
    "icon": "nestjs.jpg",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "typescript",
                "path": "./src/snippets.json"
            }
        ]
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "if git-branch-is dev; then commitlint -E HUSKY_GIT_PARAMS; fi"
        }
    },
    "scripts": {
        "vscode:prepublish": ""
    },
    "devDependencies": {
        "@types/vscode": "^1.63.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "eslint": "^8.6.0",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "typescript": "^4.5.4",
        "@vscode/test-electron": "^2.0.3",
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@types/jest": "26.0.10",
        "dot-json": "^1.0.3",
        "eslint-config-prettier": "^6.14.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "git-branch-is": "^4.0.0",
        "git-format-staged": "^2.1.0",
        "husky": "4.2.5",
        "jest": "26.4.2",
        "lint-staged": "^11.0.1",
        "prettier": "^2.1.2",
        "rimraf": "^2.6.2",
        "standard-version": "^9.3.1",
        "ts-jest": "26.2.0",
        "ts-node-dev": "^1.1.6"
    }
}
